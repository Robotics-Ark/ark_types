package _arktypes_std;

// Common message types representing primitive data.

// Wrapper message carrying a single 64-bit signed integer.
struct int64_t {

    // Integer value.
    int64_t data;
}

// Variable‑length array of floating-point values.
struct float_multiarray_t {

    // Number of valid elements in `data`.
    int64_t n;

    // Floating‑point elements.
    float data[n];
}
